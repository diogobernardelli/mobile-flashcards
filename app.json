{
  "expo": {
    "name": "mobile-flashcards",
    "slug": "mobile-flashcards",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-app.png",
    "splash": {
      "image": "./assets/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#2c3e50"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.udacifitness.exp"
    },
    "notification": {
      "icon": "https://tylermcginnis.com/udacifitness-45.png"
    },
    "description": "The best triathlon tracker ever",
    "githubUrl": "https://github.com/diogobernardelli/nanodegree_react"
  }
}